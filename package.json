{
  "name": "chatgpt-queue",
  "version": "0.1.0",
  "description": "Queue prompts and auto-send after each reply finishes.",
  "type": "module",
  "license": "GPL-3.0-only",
  "scripts": {
    "dev": "wxt dev",
    "dev:firefox": "wxt dev -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "typecheck": "tsc --noEmit",
    "test": "pnpm run typecheck && pnpm run test:storage && pnpm run test:models && pnpm run test:attachments",
    "test:storage": "tsx scripts/storage-manager-harness.ts",
    "test:models": "tsx scripts/model-menu-harness.ts",
    "test:attachments": "tsx scripts/attachments-harness.ts",
    "postinstall": "wxt prepare"
  },
  "keywords": [
    "chatgpt",
    "chrome-extension",
    "queue"
  ],
  "author": "",
  "packageManager": "pnpm@10.21.0",
  "devDependencies": {
    "@types/chrome": "^0.1.29",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "wxt": "^0.20.11"
  }
}
